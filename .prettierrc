{
	"trailingComma": "es5",
	"semi": true,
	"tabWidth": 2,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"importOrder": [
		"^@components/(.*)$",
		"^@containers/(.*)$",
		"^@ui/(.*)$",
		"^@libs/(.*)$",
		"^@utils/(.*)$",
		"^@assets/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": [
		"prettier-plugin-tailwindcss"
	]
}